{
    "name": "tanstack-react-themes",
    "version": "0.1.16",
    "description": "An abstraction for themes in your TanStack Start app",
    "author": "Mukesh Singh",
    "license": "MIT",
    "homepage": "git+https://github.com/BadgerBloke/tanstack-react-themes.git",
    "bugs": {
        "url": "https://github.com/BadgerBloke/tanstack-react-themes/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BadgerBloke/tanstack-react-themes.git"
    },
    "keywords": [
        "react",
        "tanstack",
        "theme",
        "themes",
        "tanstack-react-themes",
        "tanstack-start-themes",
        "tanstack-react-start-themes"
    ],
    "scripts": {
        "clean": "rimraf ./dist",
        "lint:package": "publint",
        "build": "pnpm run clean && pnpm run build:lib",
        "build:lib": "vite build",
        "build:llm": "tsc -p ./tsconfig.json",
        "pre:publish": "pnpm run build && pnpm run lint:package"
    },
    "packageManager": "pnpm@10.14.0",
    "type": "module",
    "types": "dist/esm/index.d.ts",
    "main": "dist/cjs/src/index.cjs",
    "module": "dist/esm/src/index.js",
    "files": [
        "dist",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@tanstack/start-server-core": "^1.131.7",
        "zod": "^4.0.17"
    },
    "devDependencies": {
        "@eslint-react/eslint-plugin": "^1.52.3",
        "@tanstack/config": "^0.20.0",
        "@tanstack/react-router": "^1.131.7",
        "@tanstack/react-start": "^1.131.7",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@vitejs/plugin-react": "^5.0.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "publint": "^0.3.12",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "rimraf": "^6.0.1",
        "vite": "^6.3.5",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "@tanstack/react-router": ">=1.131.7",
        "@tanstack/react-start": ">=1.131.7",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/src/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.cts",
                "default": "./dist/cjs/src/index.cjs"
            }
        },
        "./package.json": "./package.json"
    }
}
